<%

let samples = [
  { title:'Tuiles verticales', sample:'./samples/tile-default'},
  { title:'Tuiles horizontales', sample:'./samples/tile-horizontal'},
]
  let data = {description: 'Texte M regular 2 lignes max'};
%>

<%
for (let i = 0; i < samples.length; i++) {
  let sample = samples[i];
  let total = i > 0 ? 2 : 8;
%>
<div class="<%= prefix %>-grid-row <%= prefix %>-grid-row--gutters">
  <div class="<%= prefix %>-col-12">
    <h4><%= sample.title %></h4>
  </div>
  <%
    for (let j = 0; j < total; j++) {
  %>
  <div class="<%= prefix %>-col-xs-12 <%= prefix %>-col-sm-6 <%= (i > 0) ? prefix  +'-col-md-8 rf-col-lg-10' : prefix + '-col-md-4 rf-col-lg-3' %>">
    <%- include(sample.sample, j >= total/2 ? data : {}); %>
  </div>
  <% } %>
</div>
<%- snippet(include(sample.sample)); %>
<%- snippet(include(sample.sample, data)); %>
<% } %>
