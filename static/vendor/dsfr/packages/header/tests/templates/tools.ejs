<div class="<%= prefix %>-header__tools">
    <div class="<%= prefix %>-shortcuts">
        <ul class="<%= prefix %>-shortcuts__list">
            <%
                let item;
                for (let i = 0; i < shortcuts.items.length; i++) {
                  item = shortcuts.items[i];
            %>
            <li class="<%= prefix %>-shortcuts__item <%= prefix %>-fi-<%= item.icon %>" >
                <a class="<%= prefix %>-link" href="<%- item.href %>" ><%- item.label %></a>
            </li>
            <% } %>
        </ul>
    </div>
    <%
    switch(true) {
    case locals.searchTpl !== undefined : %>
    <%- include(searchTpl, {id: 'header-search' }); %>
    <% break;

    case locals.search !== undefined:
        search.id = search.id || 'header-search'; %>
    <%- include( '../../../search/tests/templates/search', search); %>
    <% break;
    }
    %>
</div>